## Dracula color theme

set notification-error-bg       "#ff5555" # Red
set notification-error-fg       "#f8f8f2" # Foreground
set notification-warning-bg     "#ffb86c" # Orange
set notification-warning-fg     "#282a36" # Selection
set notification-bg             "#1e1f29" # Background
set notification-fg             "#f8f8f2" # Foreground

set completion-bg               "#1e1f29" # Background
set completion-fg               "#44475a" # Comment
set completion-group-bg         "#1e1f29" # Background
set completion-group-fg         "#44475a" # Comment
set completion-highlight-bg     "#282a36" # Selection
set completion-highlight-fg     "#f8f8f2" # Foreground

set index-bg                    "#1e1f29" # Background
set index-fg                    "#f8f8f2" # Foreground
set index-active-bg             "#282a36" # Current Line
set index-active-fg             "#f8f8f2" # Foreground

set inputbar-bg                 "#1e1f29" # Background
set inputbar-fg                 "#f8f8f2" # Foreground
set statusbar-bg                "#1e1f29" # Background
set statusbar-fg                "#f8f8f2" # Foreground

set highlight-color             "#ffb86c" # Orange
set highlight-active-color      "#ff79c6" # Pink

set default-bg                  "#1e1f29" # Background
set default-fg                  "#f8f8f2" # Foreground

set render-loading              true
set render-loading-fg           "#1e1f29" # Background
set render-loading-bg           "#f8f8f2" # Foreground

#
# Recolor mode settings
#

set recolor-lightcolor          "#1e1f29" # Background
set recolor-darkcolor           "#f8f8f2" # Foreground
set recolor-keephue                 true
